{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "http://localhost:8888",
    "distDir": "../system/public"
  },
  "package": {
    "productName": "AC Overlay",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "maximize": false,
        "minimize": false,
        "unmaximize": false,
        "unminimize": false,
        "startDragging": false,
        "print": false
      }
    },
    "bundle": {
      "active": false,
      "targets": "all",
      "identifier": "computer.aesthetic.ac-overlay"
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": false,
        "title": "AC Notification Overlay",
        "width": 800,
        "x": 100,
        "y": 100,
        "transparent": true,
        "decorations": false,
        "alwaysOnTop": true,
        "skipTaskbar": true
      }
    ]
  }
}
