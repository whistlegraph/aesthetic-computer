{
  "Dirs": [
    "node_modules",
    "ssl"
  ],
  "Files": [
    "package-lock.json",
    ".env"
  ],
  "CloudConfig": {
    "Platform": "gcp",
    "ProjectID": "aesthetic-computer",
    "Zone": "us-west1-a",
    "BucketName": "aesthetic-nanos"
  },
  "RunConfig": {
    "Memory": "1G",
    "Ports": [
      "8083", "80", "443"
    ],
    "UDPPorts": ["60", "70-80", "6006"],
    "Verbose": true
  },
  "Klibs": [
    "gcp",
    "tls"
  ],
  "ManifestPassthrough": {
    "gcp": {
      "metrics": {
        "interval": "120"
      },
      "logging": {
        "log_id": "my_log"
      }
    }
  }
}