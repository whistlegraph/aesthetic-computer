{
  "name": "aesthetic-computer-code",
  "publisher": "aesthetic-computer",
  "displayName": "aesthetic.computer",
  "icon": "resources/icon.png",
  "author": "Jeffrey Alan Scudder",
  "version": "1.1.0",
  "description": "An extension for loading and running aesthetic.computer pieces in VS Code.",
  "engines": {
    "vscode": "^1.74.2"
  },
  "scripts": {
    "reload": "npm run build; npm run install:ext;",
    "build": "vsce package",
    "install:ext": "code --install-extension aesthetic-computer-code-1.1.0.vsix",
    "publish": "vsce publish minor"
  },
  "main": "./extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "aestheticComputer.runPiece",
        "title": "aesthetic.computer: Run Piece ➡️"
      },
      {
        "command": "aestheticComputer.publishPiece",
        "title": "aesthetic.computer: Publish Piece ⬆️️"
      },
      {
        "command": "aestheticComputer.enterCode",
        "title": "aesthetic.computer: Enter Code️"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "piece",
          "title": "aesthetic.computer",
          "icon": "resources/aesthetic-computer.svg"
        }
      ]
    },
    "views": {
      "piece": [
        {
          "type": "webview",
          "id": "aestheticComputer.sidebarView",
          "name": "Piece"
        }
      ]
    }
  },
  "license": "None",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitpain/aesthetic-computer.git"
  },
  "devDependencies": {
    "@vscode/vsce": "^2.19.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  }
}
