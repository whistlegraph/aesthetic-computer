{
  "$schema": "vscode://schemas/color-theme",
  "name": "Aesthetic Computer: Dark ðŸŒ‘",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1228",
    "editor.foreground": "#ffffffcc",
    "editorCursor.foreground": "#c81e64",
    "editorCursor.background": "#1a1228",
    "editor.lineHighlightBackground": "#c81e6418",
    "editor.selectionBackground": "#ff640060",
    "editor.selectionHighlightBackground": "#ff640030",
    "editor.wordHighlightBackground": "#ff640030",
    "editor.findMatchBackground": "#ff640060",
    "editor.findMatchHighlightBackground": "#ff640030",
    "editorLineNumber.foreground": "#ffffff30",
    "editorLineNumber.activeForeground": "#c81e64",
    "editorIndentGuide.background1": "#0000ff30",
    "editorIndentGuide.activeBackground1": "#0000ff50",
    "editorBracketMatch.background": "#c81e6440",
    "editorBracketMatch.border": "#c81e64",
    
    "activityBar.background": "#0f0a18",
    "activityBar.foreground": "#ffffff",
    "activityBar.activeBorder": "#c81e64",
    "activityBarBadge.background": "#c81e64",
    "activityBarBadge.foreground": "#ffffff",
    
    "sideBar.background": "#15101f",
    "sideBar.foreground": "#ffffffcc",
    "sideBarTitle.foreground": "#ffffff",
    "sideBarSectionHeader.background": "#0f0a18",
    "sideBarSectionHeader.foreground": "#c81e64",
    
    "list.activeSelectionBackground": "#c81e6450",
    "list.activeSelectionForeground": "#ffffff",
    "list.hoverBackground": "#c81e6430",
    "list.focusBackground": "#c81e6440",
    
    "statusBar.background": "#c81e64",
    "statusBar.foreground": "#ffffff",
    "statusBar.debuggingBackground": "#ff6400",
    "statusBar.noFolderBackground": "#1a1228",
    "statusBarItem.hoverBackground": "#e0287840",
    
    "titleBar.activeBackground": "#0f0a18",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#15101f",
    "titleBar.inactiveForeground": "#ffffff80",
    
    "tab.activeBackground": "#1a1228",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveBackground": "#15101f",
    "tab.inactiveForeground": "#ffffff60",
    "tab.activeBorderTop": "#c81e64",
    "tab.border": "#0f0a18",
    "editorGroupHeader.tabsBackground": "#0f0a18",
    
    "panel.background": "#15101f",
    "panel.border": "#c81e64",
    "panelTitle.activeForeground": "#c81e64",
    "panelTitle.activeBorder": "#c81e64",
    "panelTitle.inactiveForeground": "#ffffff60",
    
    "terminal.background": "#1a1228",
    "terminal.foreground": "#ffffff",
    "terminalCursor.foreground": "#c81e64",
    "terminal.ansiBlack": "#0f0a18",
    "terminal.ansiRed": "#ff6464",
    "terminal.ansiGreen": "#64ff64",
    "terminal.ansiYellow": "#ffff64",
    "terminal.ansiBlue": "#6464ff",
    "terminal.ansiMagenta": "#c81e64",
    "terminal.ansiCyan": "#64ffff",
    "terminal.ansiWhite": "#ffffff",
    "terminal.ansiBrightBlack": "#606060",
    "terminal.ansiBrightRed": "#ff8080",
    "terminal.ansiBrightGreen": "#80ff80",
    "terminal.ansiBrightYellow": "#ffff80",
    "terminal.ansiBrightBlue": "#8080ff",
    "terminal.ansiBrightMagenta": "#ff80c0",
    "terminal.ansiBrightCyan": "#80ffff",
    "terminal.ansiBrightWhite": "#ffffff",
    
    "input.background": "#0f0a18",
    "input.foreground": "#ffffff",
    "input.border": "#c81e64",
    "input.placeholderForeground": "#ffffff60",
    "inputOption.activeBorder": "#c81e64",
    
    "dropdown.background": "#0f0a18",
    "dropdown.foreground": "#ffffff",
    "dropdown.border": "#c81e64",
    
    "button.background": "#c81e64",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#e02878",
    
    "badge.background": "#c81e64",
    "badge.foreground": "#ffffff",
    
    "scrollbar.shadow": "#00000080",
    "scrollbarSlider.background": "#c81e6440",
    "scrollbarSlider.hoverBackground": "#c81e6480",
    "scrollbarSlider.activeBackground": "#c81e64c0",
    
    "minimap.background": "#15101f",
    "minimap.selectionHighlight": "#ff640060",
    
    "gitDecoration.modifiedResourceForeground": "#ffcc00",
    "gitDecoration.deletedResourceForeground": "#ff6464",
    "gitDecoration.untrackedResourceForeground": "#64ff64",
    "gitDecoration.addedResourceForeground": "#64ff64",
    "gitDecoration.conflictingResourceForeground": "#ff6400",
    
    "editorError.foreground": "#ff6464",
    "editorWarning.foreground": "#ffcc00",
    "editorInfo.foreground": "#64ffff",
    
    "focusBorder": "#c81e64",
    "foreground": "#ffffffcc",
    "widget.shadow": "#00000080",
    "selection.background": "#ff640060"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#ffff00"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ffc0cb"
      }
    },
    {
      "scope": ["constant.language", "constant.character"],
      "settings": {
        "foreground": "#ff69b4"
      }
    },
    {
      "scope": ["keyword", "keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#c81e64"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#ff6400"
      }
    },
    {
      "scope": ["storage", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c81e64"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class"],
      "settings": {
        "foreground": "#ff69b4"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#ffa500"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#ffcc80"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#87ceeb"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ff69b4"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#87ceeb"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#c0c0c0"
      }
    },
    {
      "scope": ["meta.brace"],
      "settings": {
        "foreground": "#c0c0c0"
      }
    },
    
    {
      "name": "KidLisp Comments",
      "scope": "comment.line.semicolon.kidlisp",
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "name": "KidLisp Timing",
      "scope": ["constant.numeric.timing.cycle.kidlisp", "constant.numeric.timing.delay.kidlisp"],
      "settings": {
        "foreground": "#ff6400",
        "fontStyle": "bold"
      }
    },
    {
      "name": "KidLisp Embedded Codes",
      "scope": "variable.other.embedded.kidlisp",
      "settings": {
        "foreground": "#64ff64"
      }
    },
    {
      "name": "KidLisp Painting Refs",
      "scope": "entity.name.tag.painting.kidlisp",
      "settings": {
        "foreground": "#ff6400"
      }
    },
    {
      "name": "KidLisp Rainbow/Zebra",
      "scope": "support.constant.rainbow.kidlisp",
      "settings": {
        "foreground": "#c81e64",
        "fontStyle": "bold"
      }
    },
    {
      "name": "KidLisp Graphics Functions",
      "scope": "entity.name.function.graphics.kidlisp",
      "settings": {
        "foreground": "#64ffff"
      }
    },
    {
      "name": "KidLisp Control Functions",
      "scope": "entity.name.function.control.kidlisp",
      "settings": {
        "foreground": "#c81e64"
      }
    },
    {
      "name": "KidLisp Math Functions",
      "scope": "entity.name.function.math.kidlisp",
      "settings": {
        "foreground": "#ff69b4"
      }
    },
    {
      "name": "KidLisp Logic Functions",
      "scope": "entity.name.function.logic.kidlisp",
      "settings": {
        "foreground": "#c81e64"
      }
    },
    {
      "name": "KidLisp Sound Functions",
      "scope": "entity.name.function.sound.kidlisp",
      "settings": {
        "foreground": "#ff69b4"
      }
    },
    {
      "name": "KidLisp Text Functions",
      "scope": "entity.name.function.text.kidlisp",
      "settings": {
        "foreground": "#64ffff"
      }
    },
    {
      "name": "KidLisp Mode Functions",
      "scope": "entity.name.function.mode.kidlisp",
      "settings": {
        "foreground": "#ff6400"
      }
    },
    {
      "name": "KidLisp Built-ins",
      "scope": "variable.language.builtin.kidlisp",
      "settings": {
        "foreground": "#87ceeb"
      }
    },
    {
      "name": "KidLisp Color Names",
      "scope": "support.constant.color.kidlisp",
      "settings": {
        "foreground": "#ff6400"
      }
    },
    {
      "name": "KidLisp Parentheses",
      "scope": ["punctuation.paren.open.kidlisp", "punctuation.paren.close.kidlisp"],
      "settings": {
        "foreground": "#c0c0c0"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable.readonly": "#87ceeb",
    "parameter": "#ffcc80",
    "function.declaration": "#64ffff",
    "function": "#64ffff",
    "class": "#ff69b4",
    "interface": "#ff69b4"
  }
}
