{
  "name": "session-server",
  "version": "1.0.0",
  "description": "A per session-backend implementation for aesthetic.computer.",
  "main": "session.mjs",
  "type": "module",
  "scripts": {
    "start": "node session.mjs",
    "start-do": "PORT=80 node session.mjs",
    "dev": "kill-port 8889; PORT=8889 NODE_ENV=development nodemon -I --watch session.mjs --watch chat-manager.mjs session.mjs; env nogreet=true fish",
    "test": "node --experimental-vm-modules node_modules/vitest/vitest.mjs run",
    "test:watch": "node --experimental-vm-modules node_modules/vitest/vitest.mjs",
    "create": "npx jamsocket service create session-server",
    "build": "echo Built ðŸ˜„",
    "build:jamsocket": "cp ../shared/filter.mjs filter.mjs; docker build --platform linux/amd64 --tag session-server-image .",
    "deploy": "jamsocket push session-server session-server-image",
    "spawn": "jamsocket spawn session-server",
    "spawn-token": "jamsocket spawn-token create session-server",
    "list": "jamsocket backend list",
    "logs": "f() { jamsocket logs $1; };f"
  },
  "author": "Jeffrey Alan Scudder",
  "dependencies": {
    "@geckos.io/server": "^3.0.1",
    "chokidar": "^4.0.3",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.0",
    "firebase-admin": "^13.2.0",
    "geoip-lite": "^1.4.10",
    "ip": "^2.0.1",
    "jamsocket": "^0.10.8",
    "mongodb": "^6.20.0",
    "node-fetch": "^3.3.2",
    "obscenity": "^0.4.5",
    "redis": "^4.7.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@geckos.io/client": "^3.0.2",
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.9",
    "terminal-kit": "^3.1.2",
    "vitest": "^2.1.0"
  }
}
