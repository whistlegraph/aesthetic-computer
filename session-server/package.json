{
  "name": "session-server",
  "version": "1.0.0",
  "description": "A per session-backend implementation for aesthetic.computer.",
  "main": "session.mjs",
  "type": "module",
  "scripts": {
    "start": "node session.mjs",
    "start-do": "PORT=80 node session.mjs",
    "dev": "PORT=8889 NODE_ENV=development nodemon -I --watch session.mjs session.mjs",
    "create": "npx jamsocket service create session-server",
    "build": "echo Built ðŸ˜„",
    "build:jamsocket": "docker build --platform linux/amd64 --tag session-server-image .",
    "deploy": "jamsocket push session-server session-server-image",
    "spawn": "jamsocket spawn session-server",
    "spawn-token": "jamsocket spawn-token create session-server",
    "list": "jamsocket backend list",
    "logs": "f() { jamsocket logs $1; };f"
  },
  "author": "Jeffrey Alan Scudder",
  "dependencies": {
    "@geckos.io/server": "^2.3.2",
    "chokidar": "^3.5.3",
    "dotenv": "^16.3.1",
    "fastify": "^4.19.2",
    "firebase-admin": "^12.0.0",
    "ip": "^1.1.8",
    "jamsocket": "^0.8.8",
    "obscenity": "^0.1.4",
    "redis": "^4.6.7",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "terminal-kit": "^3.1.1"
  }
}
